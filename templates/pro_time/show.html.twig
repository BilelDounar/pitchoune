{% extends 'base.html.twig' %}

{% block title %}ProTime{% endblock %}

{% block body %}
    <h1>ProTime</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ pro_time.id }}</td>
            </tr>
            <tr>
                <th>Id_pro</th>
                <td>{{ pro_time.idPro }}</td>
            </tr>
            <tr>
                <th>Jour</th>
                <td>{{ pro_time.jour }}</td>
            </tr>
            <tr>
                <th>Heure_debut</th>
                <td>{{ pro_time.heureDebut ? pro_time.heureDebut|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Heure_fin</th>
                <td>{{ pro_time.heureFin ? pro_time.heureFin|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_pro_time_index') }}">back to list</a>

    <a href="{{ path('app_pro_time_edit', {'id': pro_time.id}) }}">edit</a>

    {{ include('pro_time/_delete_form.html.twig') }}
{% endblock %}
